    <dt>TaxaAvaliacaoPadrao:</dt>
    <dd>@Html.DisplayFor(model => model.TaxaAvaliacaoPadrao)</dd>

    <dt>TaxaVendaPadrao:</dt>
    <dd>@Html.DisplayFor(model => model.TaxaVendaPadrao)</dd>
</dl>
