@model InvestCarWeb.Models.Leiloeiro


<dl class='dl-horizontal'>

    <dt>Nome:</dt>
    <dd>@Html.DisplayFor(model => model.Nome)</dd>

    <dt>Telefone:</dt>
    <dd>@Html.DisplayFor(model => model.Telefone)</dd>

    <dt>Site:</dt>
    <dd> @if (!(Model.Site == null))
